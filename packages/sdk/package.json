{
  "name": "@superserve/sdk",
  "version": "0.1.1",
  "description": "TypeScript SDK for interacting with Superserve agents",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./react": {
      "import": {
        "types": "./dist/react.d.ts",
        "default": "./dist/react.js"
      },
      "require": {
        "types": "./dist/react.d.cts",
        "default": "./dist/react.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "bun test"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@superserve/typescript-config": "workspace:*",
    "@types/bun": "^1.3.9",
    "@types/react": "^19.0.0",
    "react": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0"
  },
  "keywords": [
    "superserve",
    "ai",
    "agent",
    "sdk",
    "streaming",
    "sse",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/superserve-ai/superserve.git",
    "directory": "cli"
  },
  "homepage": "https://superserve.ai",
  "bugs": {
    "url": "https://github.com/superserve-ai/superserve/issues"
  }
}
